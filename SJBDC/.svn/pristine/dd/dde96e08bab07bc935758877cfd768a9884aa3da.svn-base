input[type=text] {
padding:0;
}
     /*fieldset 容器*/
    .containerset{ 
        padding: 0;
        width: 100%;
        border: solid 1px #ccc;
        overflow: hidden;
    }
    /*fieldset 容器中的标题*/
    .containerset legend{
        margin-left: 15px;
        font-size: 14px;    
    }
    /*fieldset 收缩状态*/
    .xstatus{    
        border-right: none;
        border-left: none;
        border-bottom: none;
        border-top: solid 1px #ccc;
        margin-bottom: 10px;
    }
    /*fieldset 原始状态*/
    .xoriginstatus
    {
        border: solid 1px #ccc;
        margin-bottom: 10px;
    }
    .geo-querylist{
        width:100%;
        height:100%;
    }
    .geo-querylist .geo-query {
        float:left;
        height:30px;
        line-height:30px;
        margin : 5px 0px;
    }
    .geo-querylist .geo-query label{
        /*width:90px;*/
        display:block;
        float:left;
        text-align : right;
        padding-right:10px;
    }
    .geo-querylist .geo-query .geo-query-content{
        width:250px;
        display:block;
        float:left;
    }
    .geo-querylist .geo-query .geo-query-content input,select{
        width: 98%;
        height: 25px;
        border: solid 1px rgb(169,169,169);
    }
    .geo-querylist .geo-query .geo-query-content .Wdate{
        width:100px;
    }