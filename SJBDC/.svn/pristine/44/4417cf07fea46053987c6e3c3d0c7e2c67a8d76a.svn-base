/*预警*/
.warning_div {
	position: fixed;
	_position: absolute;
	right: 0;
	bottom: 0;
	z-index: 15;
	width: 292px;
}

.warning-ico{
    float:right;
    width: 40px;
    height: 35px;
}
.warning-list{
    height:200px;
    float:right;
    width: 250px;
    border:1px solid #ff9d9d;
    background-color: #ffdcdc;
}
.warning-title{
    height: 33px;
    padding: 0px 10px;
    line-height: 33px;
    border-bottom:1px solid #ff9d9d;
    background-color: #ffc4c4;
}
.warning-title span{
    float: left;
    display: inline-block;
    color:#c62a2a;
}
.warning-title a{
    float: right;
}
.warning-title a img{
	margin-top:6px;
}
.warning-list ul{
    display: block;
    height: 167px;
    overflow: auto;
    scrollbar-arrow-color: #ff8989; /*图6,三角箭头的颜色*/
    scrollbar-face-color: #ff8989; /*图5,立体滚动条的颜色*/
    scrollbar-3dlight-color: #ffb9b9; /*图1,立体滚动条亮边的颜色*/
    scrollbar-highlight-color: #ffb9b9; /*图2,滚动条空白部分的颜色*/
    scrollbar-shadow-color: #ffb9b9; /*图3,立体滚动条阴影的颜色*/
    scrollbar-darkshadow-color: #ffb9b9; /*图4,立体滚动条强阴影的颜色*/
    scrollbar-track-color: #ffb9b9; /*图7,立体滚动条背景颜色*/
    scrollbar-base-color:#ffb9b9; /*滚动条的基本颜色*/
    Cursor:url(mouse.cur); /*自定义个性鼠标*/

}
.warning-list ul li{
    display: block;

    line-height: 20px;
   /* width:190px;*/
    overflow:hidden;
    text-overflow: ellipsis;
    white-space nowrap;
    border-bottom: 1px solid #ffc4c4;
    background:url(../images/pic/point.png) no-repeat 10px 15px ;
    padding: 5px 20px;
}
.warning-list ul li a{
    color: #c62a2a;
    font-size: 12px;
}
.warning-list ul li a:hover{
    color: #ff3636;
}
