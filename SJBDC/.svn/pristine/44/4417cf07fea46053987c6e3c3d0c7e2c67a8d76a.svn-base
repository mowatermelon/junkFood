}
button{font-family:microsoft yahei, Arial,微软雅黑;}
.clearfloat:after{
    display:block;
    clear:both;
    content:"";
    visibility:hidden;
    height:0;
}
.clearfloat{
    zoom:1;
}
a{
    color: #888;
    text-decoration: none;
    border:0px; 
}
a:hover{
