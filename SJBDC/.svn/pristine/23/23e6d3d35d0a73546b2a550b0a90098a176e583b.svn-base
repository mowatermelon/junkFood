

/*左侧菜单*/
.g-ml{width: 200px; color: #fff; font-size: 14px; background-color: #393f42;}
.g-ml div{ height: 56px; line-height: 56px; text-indent: 72px;color:#fff;}
.g-ml .ml-focus{color: #3498db; font-weight: bold; background-color: #141617;}

