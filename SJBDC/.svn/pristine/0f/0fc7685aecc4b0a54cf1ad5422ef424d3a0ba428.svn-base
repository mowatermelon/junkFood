.m_content_caw {
    float: left;
    width: 560px;
    height: 100%;
    margin-top: 6px;
}

.m_content_caw_mark {
    width: 100%;
    height: 28px;
}

.m_content_caw_mark_enter {
    float: left;
    height: 28px;
    line-height: 28px;
    color: #ffffff;
}

.m_content_caw_mark_more {
    float: right;
    height: 28px;
    font-size:14px;
    line-height: 28px;
    color: #ffffff;
    cursor: pointer;
}

.m_content_caw_body {
    width: 100%;
    height: 344px;
    position: relative;
}

.caw_content_shadow {
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0px;
    top: 6px;
    background-color: #8FA8CC;
    filter: alpha(opacity=10);
    -moz-opacity: 0.1;
    opacity: 0.1;
    z-index:-1;
}

.caw_content{
    width:100%;
    height:324px;
    padding-top:20px;
}

.icon_wh20{
    width:20px;
    height:20px;
    text-align: center;
    line-height: 20px;
    font-size: 14px;
    color: #ffffff;
    border-radius: 3px;
}

.q_icon{
    background-color: #3399FF;
}

.a_icon{
    background-color: #20A15F;
}

.caw{
	padding-top: 10px;
    width:530px;
    margin-left: 15px;
    border-bottom: 1px dashed #8AA0C5;
    clear: both;
}

.caw_ask{
    width:100%;
    height:20px;
    margin-top: 10px;
    margin-bottom: 18px;
}

.caw_ask>div:first-child{
    float: left;
    margin-right: 10px;
}

.caw_ask>span{  
    height:20px;
    line-height: 20px;
    float: left;
    color: #ffffff;
    cursor:pointer;
}

.caw_reply{
    width:100%;
    height:20px;
    margin-bottom: 18px;
}

.caw_reply>div:first-child{
    float: left;
    margin-right: 10px;
}

.caw_reply>p{
    float: left;
    color: #8AA0C5;
    height:20px;
    line-height: 20px;
    font-size: 14px;
    color: #8AA0C5;
}

.caw_time{
    float: right;
    position: relative;
    width: 100px;
    height:20px;
}

.caw_time_icon{
    position: absolute;
    width: 16px;
    height: 16px;
    margin: auto;
    background: url(Images/portal/gd_time.png);
}

.caw_time_content{
    font-size: 14px;
    color:#4A70AE;
    margin-left: 26px;
    line-height: 18px;
}


