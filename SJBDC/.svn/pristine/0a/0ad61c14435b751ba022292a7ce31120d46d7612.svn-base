<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <script src="js/jquery-1.9.1.min.js" type="text/javascript"></script>
    <script src="js/city.js" type="text/javascript"></script>
    <script src="js/initQuery.js" type="text/javascript"></script>
    <link type="text/css" rel="stylesheet" href="../Script/plugins/bootstrap/css/bootstrap.css"/>

    <link type="text/css" rel="stylesheet" href="../Script/plugins/bootstarp-table/bootstrap-table.css?v=${v }"/>
    <link type="text/css" rel="stylesheet" href="///Script/plugins/bootstrap/css/bootstrap.css?v=${v }"/>
    <script src="../Script/plugins/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="../Script/plugins/bootstarp-table/bootstrap-table.js" type="text/javascript"></script>
    <script src="../Script/plugins/bootstarp-table/locale/bootstrap-table-zh-CN.js" type="text/javascript"></script>
    <link type="text/css" rel="stylesheet" href="../Common/css/cityPicker.css"/>
    <script src="../Common/js/cityPicker.js" type="text/javascript"></script>
    <script src="../Common/js/city.js" type="text/javascript"></script>
    <script src="../Common/js/util.js" type="text/javascript"></script>
    <link href="../Common/css/index.css" type="text/css" rel="stylesheet"/>
    <title></title>
    <script>

        var queryParams = {
            querybox: [{
                lable: "地区选择",
                boxClass: "city-choose",
                name: "cityChoose",
                placeholder: "",
            }, {
                lable: "业务号",
                boxClass: "text-area",
                name: "ywh",
                placeholder: "请输入业务号",
            }, {
                lable: "单元号",
                boxClass: "text-area",
                name: "dyh",
                placeholder: "请输入不动产单元号",
            }],
            tableParams: {
                id: "resultList",
                url: "/SJBDC/commonController/queryPage.do?queryId=JgXnYwjgQueryMapper.queryPageBusinessInfo",
                columns: [{
                    field: 'R',
                    title: '序号',
                    width: 30,
                    align: 'center'
                }, {
                    field: 'QXMC',
                    title: '区县名称',
                    width: 100,
                    align: 'center',
                }, {
                    field: 'YWH',
                    title: '业务号',
                    width: 100,
                    align: 'center',
                }, {
                    field: 'DJLX',
                    title: '登记类型',
                    width: 80,
                    align: 'center',
                }, {
                    field: 'BDCQZH',
                    title: '不动产证号',
                    width: 200,
                    align: 'center',
                    formatter: function (v) {
                        if (v)
                            return '<span title="' + v + '">' + _util.colvalue(v, 10) + '</span>'
                        else
                            return "";
                    }
                }, {
                    field: 'BDCDYH',
                    title: '不动产单元号',
                    width: 100,
                    align: 'center',
                }, {
                    title: '详情',
                    width: 100,
                    align: 'center',
                    formatter: function (value, row, index) {
                        var btnStr = '<a href="#" style="font-size:0.8rem !important;" onclick="viewModel(\'' + row.YWBSID + '\');">查看</a>';
                        return btnStr;
                    }
                }
                ]
            }
        }

        $(function () {
            InitTable.init(queryParams);

        })
    </script>
    <style>
        .query-box {

            border: 1px solid #DBDBDB;
            margin: 0 auto 10px auto;
            padding: 10px 30px;
        }

        .query-box .box-area {
            position: relative;
            padding: 10px 0;
        }

        .query-box label {
            font-weight: normal;
            font-size: 1.1rem;
            padding: 0 8px;
            color: #363636;
        }

        .query-box .text-input {
            border: 1px solid #DBDBDB;
            padding: 4px 8px;
            width: 250px
        }

        .city-choose:after {
            content: '';
            position: absolute;
            top: 50%;
            margin-top: -2.5px;
            right: 50px;
            width: 0px;
            height: 0px;
            border: 5px solid transparent;
            border-top-color: #aaa
        }

        .query-box input:focus {
            border: 1px solid #2175DE;
        }

        .query-box .btn-area {
            text-align: center;
        }

        .btn-area .row {
            margin: 0;
        }

        .btn-area a {
            display: block;
            width: 90%;
            background: #3D88E5;
            color: #fff;
            padding: 5px 0;
            cursor: pointer;
        }

        .btn-area .col-xs-5:last-child > a {
            background: #99AEC9;
        }
    </style>
</head>
<body>
<div class="container query-container">
    <div class="query-box">
        <div class="row">

        </div>
    </div>
    <div class="fix-table">
        <table id="resultList"></table>
    </div>

</div>

</body>
</html>