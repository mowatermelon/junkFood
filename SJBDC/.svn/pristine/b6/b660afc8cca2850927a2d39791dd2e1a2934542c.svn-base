/*
    我是万恶的分割线------------------------------以上是横向菜单的CSS-----------------------------------
    */
/*nav*/
ol, ul
{
    list-style: none;
}

body, div, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, pre, code, form, fieldset, legend, input, textarea, p, blockquote, th, td
{
    margin: 0;
    padding: 0;
}

.nav
{
    float: left;
    height: 42px;
    background: url('Images/nav-bg.gif') repeat-x;
    width: 100%;
}

    .nav li
    {
        float: left;
        padding-right: 1px;
        background: url('Images/nav-line.gif') no-repeat right 0;
    }

        .nav li a
        {
            display: block;
            padding: 0 30px;
            margin-top: 1px;
            height: 41px;
            line-height: 41px;
            font-family: "微软雅黑";
            font-size: 16px;
            color: #fff;
        }

            .nav li a:hover
            {
                text-decoration: none;
                color: #fbec42;
                background: url('Images/nav-hover.gif') repeat-x;
            }

    .nav .status a
    {
        color: #fff !important;
        background: url('Images/nav-status.gif') repeat-x !important;
    }


