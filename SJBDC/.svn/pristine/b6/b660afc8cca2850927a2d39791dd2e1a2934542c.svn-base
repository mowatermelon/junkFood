.span_main, .span_main_chg, .span_other
{
    text-align: left;
    display: block;
    font-size: 14px;
    font-weight: bold;
    cursor: pointer;
   
}

.span_main
{
    color: #666;
    height: 50px;
    line-height: 53px;
    background-color: #eaedf2;
}

.span_main_chg
{
    color: #fafafa;
    height: 48px;
    line-height: 53px;
    background-color: #2CABDD;
}

.span_other
{
    height: 50px;
    line-height: 44px;
    color: #666;
    font-weight: 100;
    font-size: 14px;
    background-color: #eaedf2;
}

.leftMenu
{
    width: 188px;
    height: 100%;
    background-color: #eaedf2;
   
}

.point
{
    font-weight:bold;
    margin-left:25px;
    margin-top:10px;
}

    .leftMenu .status
    {
        color: #0574b9;
        font-weight: bold;
        background-color: #FFF;
    }

    .leftMenu .status_main
    {
       height:50px;
        color: #fafafa;
        background-color: #eaedf2;
    }

.fh_sel, .fh_unsel
{
    padding-left: 10px;
}

.fh_sel
{
    color: #FFFFFF;
}

.fh_unsel
{
    color: #B5B5B5;
}
