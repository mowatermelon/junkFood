/*
    我是万恶的分割线------------------------------以下是主界面菜单的CSS-----------------------------------
    */

#divShortCut
{
    float: right;
    text-align: right;
    padding: 0;
}

.span2
{
    top: 0;
    left: 0;
}

.row-fluid [class*="span"]
{
    display: block;
    float: left;
    width: 100%;
    min-height: 30px;
    margin-left: 0px;
    *margin-left: 0px;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}

.row-fluid .span2
{
    width: 16.66666666666667%;
    *width: 16.61347517730496%;
}

.shortcut-button
{
    margin: 0;
}

#divShortCut a:hover, a:link, a:visited
{
    text-decoration: none;
}

