package com.geostar.gtgh.portal.core.entity;

import java.util.Date;

import com.geostar.gtgh.portal.annotation.Localtion;
@Localtion("com.geostar.gtgh.portal.core.dao.WorkAnswerMapper")
public class WorkAnswer {
	private String ywbsid;

	private String theme;

	private String content;

	private String createuser;

	private Date createtime;

	private String pid;

	public String getYwbsid() {
		return ywbsid;
	}

	public void setYwbsid(String ywbsid) {
		this.ywbsid = ywbsid;
	}

	public String getTheme() {
		return theme;
	}

	public void setTheme(String theme) {
		this.theme = theme;
	}

	public String getContent() {
		return content;
	}

	public void setContent(String content) {
		this.content = content;
	}

	public String getCreateuser() {
		return createuser;
	}

	public void setCreateuser(String createuser) {
		this.createuser = createuser;
	}

	public Date getCreatetime() {
		return createtime;
	}

	public void setCreatetime(Date createtime) {
		this.createtime = createtime;
	}

	public String getPid() {
		return pid;
	}

	public void setPid(String pid) {
		this.pid = pid;
	}
}